<%= simple_form_for(@course_offering) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :course %>
    <%= f.input :section_number %>
    <%= f.input :year %>
    <%= f.input :semester %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
