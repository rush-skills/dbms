<%= simple_form_for(@course_timing) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :timing %>
    <%= f.association :course_offering %>
    <%= f.association :classroom %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
