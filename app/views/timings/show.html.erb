<%- model_class = Timing -%>
<div class="page-header">
  <h1><%=t '.title', :default => Timing %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:start) %></dt>
      <dd><pre class="prettyprint"><%= @timing.start %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:end) %></dt>
      <dd><pre class="prettyprint"><%= @timing.end %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:day) %></dt>
      <dd><pre class="prettyprint"><%= @timing.day %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              timings_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_timing_path(@timing), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              timing_path(@timing),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

